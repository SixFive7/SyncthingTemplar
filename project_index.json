      "path": "./src/SyncthingTemplar",
      "tech_stack": [".NET 10.0", "C#", "Blazor Server", "MudBlazor", "System.Net.Http", "System.Text.Json"],
      "port": 5000,
      "dev_command": "dotnet run --project src/SyncthingTemplar/SyncthingTemplar.csproj",
      "test_command": "dotnet test",
      "entry_point": "src/SyncthingTemplar/Program.cs",
      "framework": "Microsoft.NET.Sdk.Web"
