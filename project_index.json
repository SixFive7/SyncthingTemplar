    "database": null,
    "ci_cd": false
  },
  "conventions": {
    "linter": null,
    "formatter": null,
    "testing": "xunit",
    "nullable_reference_types": true,
    "implicit_usings": true
  },
  "project_structure": {
    "solution_file": "SyncthingTemplar.sln",
    "main_project": "src/SyncthingTemplar/SyncthingTemplar.csproj",
    "test_project": "tests/SyncthingTemplar.Tests/SyncthingTemplar.Tests.csproj",
    "components_path": "src/SyncthingTemplar/Components",
    "services_path": "src/SyncthingTemplar/Services",
    "models_path": "src/SyncthingTemplar/Models"
